{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": [true, "log", "warning"],
    "variable-name": false,
    "import-name": false,
    "function-name": false,
    "jsx-no-lambda": false,
    "interface-name": false,
    "no-boolean-literal-compare": true,
    "jsx-boolean-value": false,
    "object-literal-shorthand": false,
    "object-literal-sort-keys": false,
    "no-implicit-dependencies": [false, "dev"],
    "no-submodule-imports": [true, "date-fns", "react-intl", "cleave.js", "@react-google-maps/api"],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "ordered-imports": false,
    "semicolon": [false, "never"],
    "no-consecutive-blank-lines": [true, 2]
  },
  "rulesDirectory": []
}
